{
  "name": "mobx",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "mobx/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/mobx",
        "main": "mobx/src/index.ts",
        "tsConfig": "mobx/tsconfig.lib.json",
        "assets": ["mobx/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs mobx {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["mobx/**/*.ts", "mobx/package.json"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "mobx/jest.config.ts"
      }
    }
  },
  "tags": []
}
